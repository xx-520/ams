<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qfedu.ams.dao.AmsExamMapper">

    <resultMap id="AEResultMap" type="com.qfedu.ams.entity.AmsExam">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="userId" property="userid" jdbcType="INTEGER"/>
        <result column="choiceId" property="choiceid" jdbcType="VARCHAR"/>
        <result column="judgeId" property="judgeid" jdbcType="VARCHAR"/>
        <result column="shortId" property="shortid" jdbcType="VARCHAR"/>
        <result column="examNum" property="examnum" jdbcType="INTEGER"/>
        <result column="locks" property="locks" jdbcType="INTEGER"/>
        <collection property="choiceQuestionslist" javaType="list" ofType="com.qfedu.ams.entity.ChoiceQuestion"
                    resultMap="com.qfedu.ams.dao.ChoiceQuestionMapper.BaseResultMap">
        </collection>
        <collection property="judgeQuestionslist" javaType="list" ofType="com.qfedu.ams.entity.JudgeQuestion"
                    resultMap="com.qfedu.ams.dao.JudgeQuestionMapper.JQResultMap">
        </collection>
        <collection property="shortQuestionslist" javaType="list" ofType="com.qfedu.ams.entity.ShortQuestion"
                    resultMap="com.qfedu.ams.dao.ShortQuestionMapper.SQResultMap">
        </collection>
    </resultMap>

    <select id="findAll" resultMap="AEResultMap">
        select * from ams.ams_exam a, ams.choice_question c, ams.judge_question j, ams.short_question s
        choiceId in
        <foreach collection="list" item="list">
            #{list.choiceid}
        </foreach>
    </select>



</mapper>